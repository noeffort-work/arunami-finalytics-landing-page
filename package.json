{
  "dependencies": {
    "@tailwindcss/cli": "^4.1.13",
    "tailwindcss": "^4.1.13"
  },
  "scripts": {
    "css": "npx @tailwindcss/cli --input ./style.css --output ./output.css --watch",
    "css:dev": "pnpm dlx @tailwindcss/cli --input ./style.css --output ./output.css --watch",
    "css:prod": "pnpm dlx @tailwindcss/cli --input ./style.css --output ./output.css --minify --map"
  }
}
